<?xml version="1.0" encoding="UTF-8"?>
<Installer>
    <Name>QtPyVCP Installer</Name>
    <Version>0.0.1</Version>
    <Title>QtPyVCP</Title>
    <Publisher>Kcjengr</Publisher>
    <ProductUrl>https://qtpyvcp.com/</ProductUrl>

    <WizardStyle>Modern</WizardStyle>
    <InstallerWindowIcon>icon.png</InstallerWindowIcon>
    <Logo>logo.png</Logo>
    <Watermark>watermark.png</Watermark>
    <StartMenuDir>QtPyVCP</StartMenuDir>
    <TargetDir>@HomeDir@/qtpyvcp/</TargetDir>
    <RemoteRepositories>
         <Repository>
             <Url>http://repository.qtpyvcp.com/main/</Url>
         </Repository>
     </RemoteRepositories>
</Installer>
